#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/前端技术练习/vuepress-starter/node_modules/.pnpm/registry.npmmirror.com+vue-demi@0.14.8_vue@3.4.31/node_modules/vue-demi/bin/node_modules:/mnt/d/前端技术练习/vuepress-starter/node_modules/.pnpm/registry.npmmirror.com+vue-demi@0.14.8_vue@3.4.31/node_modules/vue-demi/node_modules:/mnt/d/前端技术练习/vuepress-starter/node_modules/.pnpm/registry.npmmirror.com+vue-demi@0.14.8_vue@3.4.31/node_modules:/mnt/d/前端技术练习/vuepress-starter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/前端技术练习/vuepress-starter/node_modules/.pnpm/registry.npmmirror.com+vue-demi@0.14.8_vue@3.4.31/node_modules/vue-demi/bin/node_modules:/mnt/d/前端技术练习/vuepress-starter/node_modules/.pnpm/registry.npmmirror.com+vue-demi@0.14.8_vue@3.4.31/node_modules/vue-demi/node_modules:/mnt/d/前端技术练习/vuepress-starter/node_modules/.pnpm/registry.npmmirror.com+vue-demi@0.14.8_vue@3.4.31/node_modules:/mnt/d/前端技术练习/vuepress-starter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vue-demi-switch.js" "$@"
else
  exec node  "$basedir/../../bin/vue-demi-switch.js" "$@"
fi
