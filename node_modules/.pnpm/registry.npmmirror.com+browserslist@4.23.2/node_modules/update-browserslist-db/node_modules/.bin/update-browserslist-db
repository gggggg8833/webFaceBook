#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/前端技术练习/vuepress-starter/node_modules/.pnpm/registry.npmmirror.com+update-browserslist-db@1.1.0_browserslist@4.23.2/node_modules/update-browserslist-db/node_modules:/mnt/d/前端技术练习/vuepress-starter/node_modules/.pnpm/registry.npmmirror.com+update-browserslist-db@1.1.0_browserslist@4.23.2/node_modules:/mnt/d/前端技术练习/vuepress-starter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/前端技术练习/vuepress-starter/node_modules/.pnpm/registry.npmmirror.com+update-browserslist-db@1.1.0_browserslist@4.23.2/node_modules/update-browserslist-db/node_modules:/mnt/d/前端技术练习/vuepress-starter/node_modules/.pnpm/registry.npmmirror.com+update-browserslist-db@1.1.0_browserslist@4.23.2/node_modules:/mnt/d/前端技术练习/vuepress-starter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
